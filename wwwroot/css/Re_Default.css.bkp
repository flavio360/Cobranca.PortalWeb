


/*Váriaveis com todas as cores da identidade visual*/

:root {
    /* Identidade visual */
    --white: #FFFFFF;
    --azul-primary: #0e3b66;
    --azul-secundary: #00a7b3;
    --azul-complementary: #5CC4FF;
    --azul-complementary_: rgba(19, 108, 251, 0.2);
    --azul-info: #0D45F6;
    --success: #28a745;
    --azul-info: #0D45F6;
    --warning: #ffcc00;
    --danger: #dc3545;
    --danger-background: #e7a3a0;
    /* Cores padrão para textos, descrições, backgrounds */
    --gray-1: #e1e1e6;
    --gray-2: #F4F4F4;
    --gray-3: rgba(130, 130, 130, 0.5);
    --gray-4: #DCDCE0;
    --gray-5: #e9eaed;
    --gray-desc: #8c8c8f;
    --gray-fundo: #e3e4e8;
    --gray-grafic: #C4C4C4;
    --gray-info: #515251;
    --gray-text: #A6A6A6;
    --gray-background: #E5E5E5;
    --box-shadow: rgba(0, 0, 0, 0.05) 0px 5px 15px;
    --black_desc: #413f3e;
    --black_status: #303030;
    --red_status: #de3a37;
}

/*Estilo utilizado em eventos de todas as aplicações*/
.ocultar {
    display: none
}

/*------------------------------------ Configurações padrão ----------------------------------------*/

html {
    font-size: 1rem;
}

@media (min-width: 768px) {
    html {
        font-size: 1rem;
    }
}

html {
    position: relative;
    min-height: 100%;
    width: 100%;
}


.gray_desc {
    color: var(--gray-desc);
}

.black_desc {
    color: var(--black_desc);
}

input:focus {
    outline: none;
}

.modal-body {
    max-height: 65vh;
    overflow-y: auto;
}

    .modal-body::-webkit-scrollbar {
        width: 5px;
        height: 5px;
    }

    .modal-body::-webkit-scrollbar-track {
        background: #ddddde;
        border-radius: 6px;
    }

    .modal-body::-webkit-scrollbar-thumb {
        background: #53535C;
        border-radius: 6px;
    }

