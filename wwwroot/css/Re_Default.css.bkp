/*---------------------Modal-----------------*/

.LP_Modal {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 0.3rem;
}


.LP_ModalHeader {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 1rem 1rem 0 1rem;
}

.LP_ModalFooter {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    /* padding-bottom: 1rem; */
}

/*---------------------header-----------------*/
.LP_DS_Container_header {
    display: flex;
    justify-content: space-between;
    /* margin: 1rem; */
    margin-bottom: 1rem;
    align-items: center;
}

.LP_DS_Container_header_legacy {
    display: flex;
    justify-content: space-between;
    /* margin: 1rem; */
    margin-bottom: 1rem;
    align-items: center;
    background-color: #fff;
    padding: 0.5rem;
    border-radius: 8px;
    border-top-left-radius: 0;
}

